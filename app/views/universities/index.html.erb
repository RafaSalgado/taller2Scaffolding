<p id="notice"><%= notice %></p>

<h1>Universities</h1>

<table>
  <thead>
    <tr>
      <th>Nombre universidad</th>
      <th>String</th>
      <th>Carrera</th>
      <th>Semestre</th>
      <th>Porcentaje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @universities.each do |university| %>
      <tr>
        <td><%= university.nombre_universidad %></td>
        <td><%= university.string %></td>
        <td><%= university.carrera %></td>
        <td><%= university.semestre %></td>
        <td><%= university.porcentaje %></td>
        <td><%= link_to 'Show', university %></td>
        <td><%= link_to 'Edit', edit_university_path(university) %></td>
        <td><%= link_to 'Destroy', university, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New University', new_university_path %>
